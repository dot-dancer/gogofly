{
    "swagger": "2.0",
    "info": {
        "description": "前端学Golang Web实战开发记录",
        "title": "Go-Web开发记录",
        "contact": {},
        "version": "0.0.1"
    },
    "paths": {
        "/api/v1/public/user/login": {
            "post": {
                "description": "用户登录详情描述",
                "summary": "用户登录",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户名",
                        "name": "name",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "密码",
                        "name": "password",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "登录成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "登录失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}